<%= form_with(model: job) do |form| %>
 
  

  <div>
    <%= form.label :job_title, style: "display: block" %>
    <%= form.text_field :job_title %>
  </div>

  <div>
    <%= form.label :company, style: "display: block" %>
    <%= form.text_field :company %>
  </div>

  <div>
    <%= form.label :url, style: "display: block" %>
    <%= form.text_field :url %>
  </div>

  <div>
    <%= form.label :requirment, style: "display: block" %>
    <%= form.text_field :requirment %>
  </div>

  <div>
    <%= form.label :summmary, style: "display: block" %>
    <%= form.text_area :summmary %>
  </div>

  <div>
    <%= form.label :responsibily, style: "display: block" %>
    <%= form.text_field :responsibily %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
