<div id="<%= dom_id course %>">
  <p>
    <strong>Course name:</strong>
    <%= course.course_name %>
  </p>

  <p>
    <strong>Course type:</strong>
    <%= course.course_type %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= course.price %>
